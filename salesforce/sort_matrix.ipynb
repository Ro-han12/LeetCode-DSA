    "def median(arr):\n",
    "    if len(arr)%2==0:\n",
    "        i=len(arr)//2\n",
    "        z=arr[i]\n",
    "        a=arr[i-1]\n",
    "        ans=(z+a) / 2 \n",
    "        return ans \n",
    "    else:\n",
    "        z=len(arr)//2\n",
    "        ans=arr[z]\n",
    "        return ans \n",
    "    \n",
    "arr1=[1,3]\n",
    "arr2=[2]\n",
    "arr3=arr1+arr2\n",
    "arr3.sort()\n",
    "arr3\n",
    "median(arr3)"
   ]
  }
